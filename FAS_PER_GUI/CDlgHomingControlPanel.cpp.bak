void CDlgHomingControlPanel::OnBnClickedMfcbuttonClose()
{
	if (TRUE == AskClose())
	{
		for (INT nAxis = 1; nAxis < 9; nAxis++)
		{
			if (m_pDevice->IsHoming(nAxis))
			{
				m_pDevice->MoveStop(nAxis);
			}
		}

		OnOK();
	}
}

